{
  "name": "@yunti-private/yt-frontend-ui",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "prepare": "husky install",
    "docs:build": "dumi build",
    "docs:deploy": "npm publish",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,css,less,scss,html,json,md}'",
    "eslint:fix": "eslint --fix --ext '**/*.{js,jsx,ts,tsx}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,less,scss,html}' --allow-empty-input",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,scss,css,html}": [
      "npm run stylelint:fix",
      "prettier --write"
    ],
    "*.js?(x)": [
      "npm run eslint:fix",
      "prettier --write"
    ],
    "*.ts?(x)": [
      "npm run eslint:fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "antd": "^4.18.8",
    "babel-plugin-import": "^1.13.3",
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.3",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0"
  }
}
